{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app/**/*",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "format": "prettier --ignore-path .prettierignore --check .",
    "docker:dev": "docker-compose up --build -d",
    "docker:restart": "docker-compose down && docker-compose up --build -d && docker-compose logs -f",
    "docker:down": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "prisma:studio": "npx prisma studio",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres && npx prisma migrate dev --name reset"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.2.1",
    "@remix-run/node": "^2.16.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "axios": "^1.8.4",
    "es-toolkit": "^1.34.1",
    "isbot": "^5.1.21",
    "my-remix-app": "link:",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@remix-run/dev": "^2.15.2",
    "@types/axios": "^0.14.4",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "sass": "^1.83.4",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "~20.17.0"
  }
}

{
  "name": "my-remix-app",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app/**/*",
    "start": "remix-serve ./build/server/index.js",
    "typecheck": "tsc",
    "format": "prettier --ignore-path .prettierignore --check .",
    "docker:dev": "docker-compose up --build -d",
    "docker:restart": "docker-compose down && docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "studio:dev": "npx prisma studio",
    "studio:prod": "kubectl port-forward service/prisma-studio-service 5555:5555",
    "build:scripts": "tsup \"scripts/**/*.ts\" \"app/external/music/**/*.ts\" --format esm --out-dir dist --minify --clean --splitting=false --platform=node --target=node20 --treeshake --external @prisma/client,pg,pg-native,pg-query-stream"
  },
  "dependencies": {
    "@blocknote/core": "^0.35.0",
    "@blocknote/mantine": "^0.35.0",
    "@blocknote/react": "^0.35.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/client": "^6.2.1",
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.17.0",
    "@remix-run/router": "^1.23.0",
    "@remix-run/serve": "^2.15.2",
    "@remix-run/server-runtime": "^2.17.0",
    "axios": "^1.8.4",
    "compression": "^1.8.0",
    "cookie": "^1.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.3",
    "es-toolkit": "^1.34.1",
    "express": "^5.1.0",
    "github-markdown-css": "^5.8.1",
    "isbot": "^5.1.21",
    "jose": "^6.0.12",
    "minio": "^8.0.6",
    "morgan": "^1.10.0",
    "my-remix-app": "link:",
    "nprogress": "^0.2.0",
    "pg": "^8.16.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-responsive": "^10.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "remark-gfm": "^4.0.1",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "remix-utils": "^8.8.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@remix-run/dev": "^2.15.2",
    "@types/axios": "^0.14.4",
    "@types/cookie": "^1.0.0",
    "@types/node": "^24.7.2",
    "@types/nprogress": "^0.2.3",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "sass": "^1.83.4",
    "tsup": "^8.5.0",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "pnpm@9.10.0",
  "engines": {
    "node": "~20.17.0"
  }
}
